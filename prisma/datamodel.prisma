type User {
  id: ID! @unique
  email: String! @unique
  name: String! @unique
}

type Pedal {
  id: ID! @unique
  builder: User
  name: String!
  width: Float!
  height: Float!
  knobs: [Knob!]! @relation(name: "ListKnobsOnPedal")
}

type Knob {
  id: ID! @unique
  type: String!
  description: String!
  cx: Float
  cy: Float
  r: Float
  angle: Float
  width: Float
  pedal: Pedal @relation(name: "ListKnobsOnPedal")
}
