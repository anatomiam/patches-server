type User {
  id: ID! @unique
  email: String @unique
  name: String!
  knobs: [Knob!]!
}

type Pedal {
  id: ID! @unique
  name: String!
  dimensions: Dimensions!
  selectedComponent: String
  knobs: [Knob!]! 
}

type Dimensions {
  id: ID! @unique
  width: Float!
  height: Float!
}

type Knob {
  id: ID! @unique
  type: String!
  description: String!
  builder: User
  cx: Float
  cy: Float
  r: Float
  angle: Float
  width: Float
  pedal: Pedal
}
